            "externalConsole": false,
            // "preLaunchTask": "source_setup",
            // "postDebugTask": "close_terminal",
            "MIMode": "gdb",
            "avoidWindowsConsoleRedirection": true, //must add 
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
